<div class="container">
  <div class="row">
    <div class="col-12">
      <h1><%= t('.header') %></h1>
    </div>
  </div>
  <div class="row">
    <div class="col-12">
      <table class="table">
        <thead>
          <tr>
            <th class="sort-by-title pointer">
              <%= t('.title') %>
              <%= octicon 'arrow-up', class: 'text-success' %>
              <%= octicon 'arrow-down', class: 'text-success hide' %>
            </th>
            <th><%= t('.level') %></th>
            <th><%= t('.questions') %></th>
            <th><%= t('.action') %></th>
          </tr>
        </thead>
        <tbody>
          <%= render @tests %>
        </tbody>
      </table>
    </div>
  </div>
</div>
